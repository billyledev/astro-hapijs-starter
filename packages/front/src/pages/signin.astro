---
import DefaultLayout from '@layouts/Default.astro';
import SigninForm from '@components/forms/Signin.vue';

const title = 'Signin';
---

<DefaultLayout title={title}>
  <main class="py-16 space-y-16">
    <div class="w-full md:w-2/3 lg:w-1/2 xl:w-1/3 mx-auto px-4 text-center">
      <h2 class="pb-8">{title}</h2>
      <SigninForm client:only="vue"/>
    </div>
  </main>
</DefaultLayout>
